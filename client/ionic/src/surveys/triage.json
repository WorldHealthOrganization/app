{
 "title": "Health Survey",
 "completedHtmlOnCondition": [
  {
   "expression": "{question1} anyof ['item1', 'item2', 'item3']",
   "html": "You may not necessarily have CORVID-19, but please follow the following steps: <br>Step 1: Please isolate yourself from others, read the <a href=/self-isolation> TODO link self isolation guide </a> <br> Step 2: call your doctor"
  },
  {
   "expression": "{question2} notempty",
   "html": "There is a low probability that these symptoms relate to CORVID-19, but please follow the following steps to protect yourself and others: <br>Step 1: Please isolate yourself from others, read the <a href=/self-isolation> TODO link self isolation guide </a> <br> Step 2: call your doctor"
  }
 ],
 "pages": [
  {
   "name": "page1",
   "elements": [
    {
     "type": "checkbox",
     "name": "question1",
     "title": "Are you experiencing any of the following symptoms?",
     "isRequired": true,
     "choices": [
      {
       "value": "item1",
       "text": "Fever"
      },
      {
       "value": "item2",
       "text": "Cough"
      },
      {
       "value": "item3",
       "text": "Difficulty Breathing"
      },
      {
       "value": "item5",
       "text": "Other"
      }
     ]
    },
    {
     "type": "checkbox",
     "name": "question2",
     "visibleIf": "{question1} contains 'item5'",
     "title": "Are you experiencing any of the following symptoms?",
     "choices": [
      {
       "value": "item1",
       "text": "Muscle Pain"
      },
      {
       "value": "item2",
       "text": "Nasal Congestion"
      },
      {
       "value": "item3",
       "text": "Sore Throat"
      },
      {
       "value": "item4",
       "text": "Headache"
      }
     ]
    },
    {
     "type": "text",
     "name": "question3",
     "visibleIf": "{question1} notempty or {question2} notempty",
     "title": "When did these symptoms start",
     "inputType": "date"
    },
    {
     "type": "checkbox",
     "name": "question7",
     "title": "Do you have the following pre-existing medical conditions",
     "choices": [
      {
       "value": "item1",
       "text": "Heart Disease"
      },
      {
       "value": "item2",
       "text": "Diabetes"
      },
      {
       "value": "item3",
       "text": "Lung Disease"
      }
     ]
    },
    {
     "type": "text",
     "name": "question5",
     "visibleIf": "{question1} notempty",
     "title": "Your Age",
     "validators": [
      {
       "type": "numeric",
       "text": "must be a number"
      }
     ],
     "inputType": "number"
    },
    {
     "type": "dropdown",
     "name": "question6",
     "title": "Your Country",
     "choices": [
      "item1",
      "item2",
      "item3"
     ]
    }
   ]
  }
 ],
 "triggers": [
  {
   "type": "complete"
  },
  {
   "type": "complete"
  }
 ],
 "showPageTitles": false,
 "showQuestionNumbers": "off"
}